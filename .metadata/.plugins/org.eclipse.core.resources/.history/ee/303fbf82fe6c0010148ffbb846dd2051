	@Autowired
	private JdbcTemplate jdbcTemplate;
//	JdbcTemplate provide method to interact with database.
	
	public void insertUser(User user) {
		String INSERT_SQL_QUERY = "INSERT INTO user(name, email, gender, city) VALUES (?,?,?,?)";
	}

}
